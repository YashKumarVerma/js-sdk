{
  "name": "@kintone/plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "3.0.12",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "browser": "dist/index.min.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "types": "lib/index.d.ts",
  "files": [
    "bin",
    "lib",
    "dist",
    "from-manifest.js"
  ],
  "scripts": {
    "build": "npm run tsc && npm run build:site && npm run rollup",
    "build:site": "run-p js css",
    "lint": "npm run eslint",
    "test": "run-p jest site:test",
    "test:ci": "npm run test",
    "css": "postcss --config postcss.config.js ../../node_modules/normalize.css/normalize.css > docs/dist/normalize.min.css",
    "eslint": "eslint \"*.{js,ts}\" bin src site test",
    "js": "webpack --mode production",
    "js:dev": "webpack serve --mode development",
    "jest": "jest",
    "prerelease": "run-p lint test build",
    "site": "run-p js css",
    "site:dev": "run-p css js:dev",
    "site:test": "jest --config site/jest.config.js",
    "tsc": "rm -rf ./lib && tsc",
    "rollup": "run-p rollup:*",
    "rollup:prod": "rollup -c --environment BUILD:production",
    "rollup:dev": "rollup -c --environment BUILD:development"
  },
  "dependencies": {
    "@kintone/plugin-manifest-validator": "^3.0.12",
    "chokidar": "^3.5.1",
    "core-js": "^3.8.3",
    "debug": "^4.3.1",
    "denodeify": "^1.2.1",
    "meow": "^9.0.0",
    "mkdirp": "^1.0.4",
    "node-rsa": "~1.1.1",
    "stream-buffers": "^3.0.2",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/stream-buffers": "^3.0.3",
    "@types/yauzl": "^2.9.1",
    "@types/yazl": "^2.4.2",
    "ajv": "^6.12.6",
    "array-flatten": "^3.0.0",
    "assert": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-replace-ts-export-assignment": "^0.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "cssnano": "^4.1.10",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "normalize.css": "^8.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "process": "^0.11.10",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-polyfill-node": "^0.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "setimmediate": "^1.0.5",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.3",
    "webpack": "^5.20.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/plugin-packer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/plugin-packer"
  },
  "bugs": "https://github.com/kintone/js-sdk/issues",
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
